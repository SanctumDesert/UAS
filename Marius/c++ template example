
#include <iostream>

using namespace std;
template <size_t arraLenght>
void printArray(int (&a)[arraLenght]){
	cout << "size of array : " << arraLenght << endl;
	for(int i = 0; i < arraLenght; i++){
		cout << a[i] << endl;
	}
}

int main(){
	int a[] = {1,3,4,2,5,10};
	printArray(a);
	return 0;
}


